{
  "name": "cares",
  "version": "1.1.8",
  "description": "node.js interface to c-ares library",
  "repository": {
    "type": "git",
    "url": "https://github.com/royalpinto/node-cares.git"
  },
  "keywords": [
    "dns",
    "c-ares",
    "cares",
    "ares",
    "resolver"
  ],
  "main": "lib/cares.js",
  "dependencies": {
    "nan": "^2.14.0",
    "q": "^1.5.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.5.1",
    "eslint-config-google": "^0.14.0",
    "jsdoc-to-markdown": "^5.0.2",
    "mocha": "^6.2.1",
    "native-dns": "^0.7.0"
  },
  "scripts": {
    "test": "mocha test",
    "install": "node-gyp rebuild --release",
    "lint": "eslint .",
    "docs": "jsdoc2md lib/*.js > API.md"
  },
  "author": "Royal Pinto <royalpinto@gmail.com>",
  "license": "MIT",
  "gypfile": true,
  "eslintConfig": {
    "extends": "google",
    "rules": {
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "no-multiple-empty-lines": [
        2,
        {
          "max": 2,
          "maxEOF": 1
        }
      ],
      "no-var": [
        0
      ],
      "prefer-rest-params": [
        0
      ],
      "indent": [
        2,
        4,
        {
          "SwitchCase": 1
        }
      ]
    }
  }
}
